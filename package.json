    "jasmine": "^3.5.0",
    "jsonfile": "^6.0.1",
    "nodemon": "^2.0.3",
    "supertest": "^4.0.2",
    "ts-node": "^8.10.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.2",
    "typescript": "^3.9.5"
